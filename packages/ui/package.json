{
  "name": "@package-inspector/ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "yarn build:graphql && next dev",
    "build": "yarn build:graphql && next build",
    "build:nexus": "ts-node-dev -O '{ \"module\": \"commonjs\" }' --transpile-only graphql/schema.ts",
    "build:graphql": "yarn build:nexus && yarn build:graphql-codegen",
    "build:graphql-codegen": "graphql-codegen",
    "start": "next start",
    "lint": "yarn build:nexus && next lint",
    "test:ci": "echo 'TESTING NOT AVAILABLE YET'"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.4.0",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "@nivo/core": "^0.79.0",
    "@nivo/treemap": "^0.79.1",
    "@package-inspector/core": "*",
    "apollo-server-micro": "^3.6.7",
    "classnames": "^2.3.1",
    "graphql": "^16.4.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.1.5",
    "nexus": "^1.3.0",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.14",
    "@graphql-tools/utils": "^8.6.9",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.27",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.6.3"
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.10"
  }
}
