{
  "name": "@package-inspector/ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "yarn build:nexus && next dev",
    "build": "yarn build:nexus && next build",
    "build:nexus": "ts-node-dev -O '{ \"module\": \"commonjs\" }' --transpile-only graphql/schema.ts && prettier --write graphql/generated/*",
    "start": "next start",
    "lint": "yarn build:nexus && next lint"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@nextui-org/react": "^1.0.0-beta.6",
    "@nivo/core": "^0.79.0",
    "@nivo/treemap": "^0.79.1",
    "@package-inspector/core": "*",
    "@package-inspector/plugin-preset": "*",
    "apollo-server-micro": "^3.6.7",
    "classnames": "^2.3.1",
    "graphql": "^16.4.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.1.5",
    "nexus": "^1.3.0",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@graphql-tools/utils": "^8.6.9",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.27",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.6.3"
  },
  "volta": {
    "node": "16.14.2",
    "yarn": "1.22.10"
  }
}
